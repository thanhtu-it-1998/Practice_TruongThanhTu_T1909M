@{
    ViewData["Title"] = "Home Page";
}

<table class="table">
    <thead>
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
            <th scope="col">Face</th>
            <th scope="col">Department</th>
            <th scope="col">Phone</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var e in Model)
        {
        <tr>
            <th scope="row">@e.ID</th>
            <td>@e.Name</td>

            <td>
                <img src="~/Img/@e.Image" />
            </td>
            <td>@e.Department</td>
            <td>@e.Phone</td>
        </tr>
        }
        

    </tbody>
</table>
